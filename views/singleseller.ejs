

   <!-- single Seller view  -->
   <div class="single">
    <div class="container">
        <div class="row justify-content-center">
          <div class="col-md-10">
            <div class="card shadow">
                <a href="/seller"> <button class="btn btn-sm btn btn-primary mb-4">Back</button></a>
              <div class="card-body" style="display: block;">
                <div class="row">
                    <div class="col-md-6">
                        <img src="/seller/<%=seller.photo %>" alt="">   
                    </div>
                    <div class="col-md-6">
                        <h3><%=seller.name%></h3>
                        <p>Phone: <%= seller.phone %></p>
                        <p>Email: <%= seller.email %> </p>
                        <p>Product-type: <%= seller.product %></p>
                    </div>
                </div>  
              </div>
            </div>
          </div>
        </div>
      </div>
  </div>
 


 
  